@using Entity_Layer.Concrete
@model List<Experience> 
<div class="row ">
    <div class="col-12 grid-margin">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Deneyimli Olunan Programlama Dilleri</h4>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th> Başlık </th>
                                <th> Unvan </th>
                                <th> Açıklama </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach(var item in Model){
                            <tr>
                                <td>
                                    <img src="~/argon-dashboard-master/assets/img/theme/csharp.jpg" alt="" />
                                    <span class="pl-2">@item.experienceTitle</span>
                                </td>
                                <td> @item.experienceDate </td>
                                <td> @item.experienceDescription </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>