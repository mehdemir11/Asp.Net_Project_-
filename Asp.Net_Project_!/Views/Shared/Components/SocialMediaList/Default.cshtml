@using Entity_Layer.Concrete
@model List<SocialMedia>

<div class="social-nav" data-aos="fade-up" data-aos-delay="200">
    <nav role="navigation">
        <ul class="nav justify-content-left">

            @foreach (var item in Model)
            {
                <li class="nav-item"><a class="nav-link" href="@item.socialMediaUrl" title="@item.socialMediaTitle"><i class="@item.socialMediaIcon1"></i><span class="menu-title sr-only">@item.socialMediaTitle</span></a></li>
            }
        </ul>
    </nav>
</div>